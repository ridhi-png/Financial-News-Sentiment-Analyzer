      "cell_type": "code",
      "execution_count": 2,
      "metadata": {
        "id": "K2WLWa8UYSxj",
        "colab": {
          "base_uri": "https://localhost:8080/"
        },
        "outputId": "2ea957a7-e7e2-4525-d72d-487fde9470e1"
